toolcall tool name=a1111_txt2img id=images prompt="minimal abstract wallpaper, soft gradient sky, geometric shapes, cinematic lighting, high detail" negative="text, watermark, logo, lowres, blurry" width=1920 height=1080 steps=25 cfg=6.5 seed=42 n=6 sampler="Euler a"
